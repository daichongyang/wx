/* components/select_item.wxss */
.header{
  position: fixed;
  top: 10rpx;
  display: -webkit-flex; /* Safari */
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 60rpx;
  align-items: center;
  background: white;
  left: 0;
  right: 0;
  border-bottom: 1rpx solid #e1e1e1
}
.xqCssBox{
  position: fixed;
  top: 80rpx;
  width: 100%;
  height: 180rpx;
  background: white;
}
.header text{
  text-align: center;
  flex-grow: 1;
  font-size: 30rpx;
  border-right: 1rpx solid #e1e1e1;
}
.maskView{
  width: 100%;
  position: fixed;
  top: 75rpx;
  bottom: 0;
  background: rgba(1, 1, 1, 0.3);
}

.xsItem{
  background: white;
  text-align: center;
  padding: 10rpx;
  border-bottom: 1rpx solid #f1f1f1;
  font-size: 30rpx;
  color: darkgrey
}