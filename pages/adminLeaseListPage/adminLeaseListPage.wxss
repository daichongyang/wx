}

.scrollView{
  position: fixed;
  top: 85rpx;
  bottom: 0;
}

/* 预约的item */
.itemBox{
  margin-top: 20rpx;
}
.titel{
  padding-left: 30rpx;
  height: 100rpx;
  background: white;
  font-size: 30rpx;
}
.houseName {
  font-size: 30rpx;
  color: #222222;
