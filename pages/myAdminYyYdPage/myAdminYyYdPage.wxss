/* pages/myAdminYyYdPage/myAdminYyYdPage.wxss */
page{
 background: #f7f7f7; 
}

.titelBox{
  margin-top: 10rpx;
  height: 80rpx;
  background: white;
}
.TitleItemCss{
  display: -webkit-flex; /* Safari */
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.TitleItemCss text:nth-child(1){
  display: inline-block;
  text-align: center;
   flex-grow: 1;
  font-size: 30rpx;
  line-height: 80rpx;
}
.TitleItemCss text:nth-child(2){
  display: inline-block;
  text-align: center;
  flex-grow: 1;
  font-size: 30rpx;
  line-height: 80rpx;
}

.itemOne{
  border-bottom: 5rpx solid #1fc2b2;
}

.swiper{
  position: fixed;
  top: 95rpx;
}
.scrollView{
  width: 100%;
  height: 100%;
}

/* 预约的item */
.itemBox{
  margin-top: 20rpx;
}
.titel{
  padding-left: 30rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  background: white;
  font-size: 30rpx;
}
.content_container {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.content{
  padding-left: 30rpx;
  color: #777777;
  font-size: 26rpx;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
}
.contentType{
  display: inline-block;
  background: #dff3f2;
  color: #619793;
  text-align: center;
  font-size: 20rpx;
  margin-left: 30rpx;
  padding: 5rpx 20rpx;
  border-radius: 8rpx;
  margin-bottom: 8rpx;
}

.foot{
  width: 100%;
  height: 100rpx;
  background: white;
}
.foot text{
  float: right;
  font-size: 26rpx;
  background: #dff3f2;
  color: #4f8e8a;
  height: 60rpx;
  margin-top: 20rpx;
  margin-right: 30rpx;
  line-height: 60rpx;
  width: 150rpx;
  text-align: center;
  border-radius: 8rpx;
}

.footerLoad{
  padding: 20rpx;
  text-align: center;
  font-size: 25rpx;
  color: darkgray;
}

.titleRightOne{
  float: right;
  margin-right: 30rpx;
  color: #00c3b2;
}
.titleRightTwo{
  float: right;
  margin-right: 30rpx;
  color: #fe615a;
}