             <view class='mxItem'><text>今天</text><text>+7562.22</text><text>-56.76</text></view>
             <view class='mxItem'><text>03-31</text><text>+89274.66</text><text>-7899.81</text></view>
          </block>
        </view>


    </scroll-view>
  </swiper-item>


</swiper>
